{
  "name": "api-nabaztag",
  "version": "0.0.1",
  "description": "API Nabaztag",
  "license": "MIT",
  "author": "Antoine Aumjaud",
  "scripts": {
    "build-config": "./build-config.sh",
    "build-tsc": "tsc",
    "build-tsc-mon": "tsc -w",
    "build": "npm run build-config && npm run build-tsc",
    "debug": "npm run build && node --inspect dist/server.js",
    "start": "npm run build && node dist/server.js",
    "start-mon": "npm run build-config && nodemon -e ts  --exec \"npm run build-tsc && node dist/server.js\"",
    "test": "npm run build && jasmine --config=test/jasmine.json"
  },
  "main": "src/server.js",
  "repository": "https://github.com/antoine-aumjaud/api-nabaztag",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.36",
    "@types/node": "^7.0.31",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.4"
  }
}
